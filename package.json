{
  "name": "chord-viewer",
  "version": "0.0.0",
  "author": "Cédric Néhémie <cedric.nehemie@gmail.com>",
  "description": "Chord viewer for HTML",
  "keywords": [
    "chord",
    "viewer",
    "webcomponent"
  ],
  "repository": {
    "url": "https://github.com/abe33/chord-viewer.git"
  },
  "license": "MIT",
  "engine": {
