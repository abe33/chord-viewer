    "expect.js": "^0.3.1",
    "mocha": "^2.3.4",
    "standard": "^5.4.1",
    "esdoc": "^0.4.0"
  },
  "standard": {
    "globals": [
      "it",
      "describe",
      "beforeEach"
    ]
  },
  "babel": {
